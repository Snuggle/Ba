    "typeorm-seeding": "^1.6.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.0",
    "@types/faker": "^5.1.5",
    "@types/node": "^14.11.10",
